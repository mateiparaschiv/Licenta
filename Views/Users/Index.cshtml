@model List<UserModel>
@{
    ViewData["Title"] = "Index Users";
    //TODO : DELETE PAGE
}

<h1>Index Users</h1>
<table class="table table-bordered table-striped" style="width:100%">
    <thead>
        <tr>
            <th>
                User id
            </th>
            <th>
                User name
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach(var obj in Model)
        {
            <tr>
                <td width="50%">
                    @obj.Password
                </td>
                <td width="50%">
                    @obj.UserName
                </td>
            </tr>
        }
    </tbody>
</table>