@model List<ReviewModel>
@{
    ViewData["Title"] = "Index Reviews";
}

<h1>Index Reviews</h1>
<table class="table table-bordered table-striped" style="width:100%">
    <thead>
        <tr>
            <th>
                Review id
            </th>
            <th>
                Review message
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var obj in Model)
        {
            <tr>
                <td width="50%">
                    @obj.Id
                </td>
                <td width="50%">
                    @obj.Message
                </td>
            </tr>
        }
    </tbody>
</table>