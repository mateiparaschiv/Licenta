@model List<ArtistModel>
@{
    ViewData["Title"] = "Index Artist";
}

<h1>Index Artists</h1>
<table class="table table-bordered table-striped" style="width:100%">
    <thead>
        <tr>
            <th>
                Artist id
            </th>
            <th>
                Artist name
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var obj in Model)
        {
            <tr>
                <td width="50%">
                    @obj.Id
                </td>
                <td width="50%">
                    @obj.Name
                </td>
            </tr>
        }
    </tbody>
</table>